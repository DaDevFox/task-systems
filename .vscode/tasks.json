{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "Build InventoryClient frontend",
			"command": "dotnet build inventory-core/frontend/InventoryClient.sln",
			"group": "build",
			"problemMatcher": [
				"$msCompile"
			]
		},
		{
			"type": "shell",
			"label": "Run InventoryClient frontend",
			"command": "dotnet run --project inventory-core/frontend/InventoryClient/InventoryClient.csproj",
			"group": "build",
			"problemMatcher": [
				"$msCompile"
			]
		}
	]
}